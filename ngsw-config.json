{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dynamicResources",
      "urls": [
        "/**",
        "/**/*.jpg",
        "/**/*.png"
      ],
      "cacheConfig": {
        "maxSize": 250,
        "maxAge": "20d"
      }
    }
  ]
}
